{
  "name": "@would-you/types",
  "version": "0.0.1",
  "description": "Typings for Would You.",
  "keywords": [
    "typescript",
    "types",
    "would-you",
    "discord"
  ],
  "homepage": "https://github.com/Would-You-Bot/types#readme",
  "bugs": {
    "url": "https://github.com/Would-You-Bot/types/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Would-You-Bot/types.git"
  },
  "license": "MIT",
  "author": "Slekup",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {},
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run lint:fix && npm run format && npm run build:nocheck",
    "build:nocheck": "rimraf dist && tsup",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint src/**/* --ext .ts && tsc --noEmit",
    "lint:fix": "eslint --fix src/**/* --ext .ts && tsc --noEmit",
    "precommit": "npm run lint:fix && npm run format",
    "prepare": "husky install"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "printWidth": 80,
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.5",
    "@types/node": "^20.3.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.2.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-prettier": "^0.0.0-empty",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.3"
  }
}
